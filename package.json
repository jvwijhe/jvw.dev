{
  "name": "jvw.dev",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "storyblok-pull": "storyblok login && storyblok pull-components --space 285943",
    "storyblok-generate": "storyblok-generate-ts source=./components.285943.json target=./src/lib/storyblok/types",
    "storyblok": "npm run storyblok-pull && npm run storyblok-generate",
    "prepare": "husky install",
    "check:unused": "lilmolly"
  },
  "devDependencies": {
    "@playwright/test": "^1.28.1",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/adapter-cloudflare": "^4.4.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/eslint": "8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "sveltekit-superforms": "^2.12.5",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.3",
    "vitest": "^1.2.0"
  },
  "type": "module",
  "dependencies": {
    "@sentry/sveltekit": "^7.112.0",
    "@storyblok/svelte": "^4.0.6",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "aos": "^3.0.0-beta.6",
    "autoprefixer": "^10.4.17",
    "axios": "^1.6.8",
    "class-variance-authority": "^0.7.0",
    "postcss": "^8.4.33",
    "storyblok": "^3.30.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^3.4.1"
  }
}
